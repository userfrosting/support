{
    "name": "userfrosting/support",
    "type": "library",
    "description": "Support classes for UserFrosting and UF modules",
    "keywords": ["userfrosting", "support", "exception"],
    "homepage": "https://github.com/userfrosting/support",
    "license" : "MIT",
    "authors" : [
        {
            "name": "Alexander Weissman",
            "homepage": "https://alexanderweissman.com"
        }
    ],
    "require": {
        "illuminate/config": "^5.8",
        "jackiedo/dotenv-editor": "~1.0.7",
        "php": ">=7.1",
        "userfrosting/uniformresourcelocator": "~4.2.1 | ~4.3.0 | ~4.4.0 | ~4.5.0",
        "symfony/yaml": ">=2.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.13",
        "phpunit/phpunit": "^7.5 | ^8.5"
    },
    "autoload": {
        "psr-4": {
            "UserFrosting\\Support\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "UserFrosting\\Support\\Tests\\": "tests"
        }
    }
}
